<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="183.0" prefWidth="384.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.VerificationController">
   <children>
      <Label fx:id="Title" alignment="CENTER" layoutX="1.0" layoutY="36.0" prefHeight="41.0" prefWidth="419.0" text="Are you sure you want to delete this email?">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Button layoutX="131.0" layoutY="143.0" mnemonicParsing="false" onAction="#cancel" prefHeight="26.0" prefWidth="54.0" style="-fx-background-color: #152F90;;" text="No" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="236.0" layoutY="143.0" mnemonicParsing="false" onAction="#delete" prefHeight="26.0" prefWidth="54.0" style="-fx-background-color: FD0000;;" text="Yes" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
